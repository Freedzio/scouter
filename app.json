{
	"expo": {
		"scheme": "scouter",
		"name": "Scouter",
		"slug": "scouter",
		"owner": "Scouter",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"splash": {
			"image": "./assets/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "scouter",
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/logo.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.freedzio.scouter"
		},
		"web": {
			"favicon": "./assets/logo.png"
		}
	}
}
